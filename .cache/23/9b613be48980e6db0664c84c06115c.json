ource":"public/css/home.scss","name":null,"original":{"line":366,"column":8},"generated":{"line":286,"column":2}},{"source":"public/css/home.scss","name":null,"original":{"line":367,"column":8},"generated":{"line":287,"column":2}},{"source":"public/css/home.scss","name":null,"original":{"line":368,"column":8},"generated":{"line":288,"column":2}}],"sources":{"public/css/home.scss":"@import url('https://fonts.googleapis.com/css?family=Montserrat:300,600&display=swap');\n@import url('https://fonts.googleapis.com/css?family=Source+Code+Pro:400,700&display=swap');\n\nbody {\n    background: #beecfa;\n    margin: 0;\n}\n\n* {\n    box-sizing: border-box;\n}\n\n.row {\n    max-width: 1400px;\n    padding: 0 24px;\n    margin: 0 auto;\n    width: 100%;\n}\n\n// .main {\n//     @media(max-width: 767px) {\n//         display: none;\n//     }\n// }\n\n// .no-desktop {\n//     @media(min-width: 768px) {\n//         display: none;\n//     }\n// }\n\nbody {\n\n    &.is-menu-open {\n        .header__menu-dropdown {\n            opacity: 1;\n            visibility: visible;\n        }\n\n    }\n\n    &.is-menu-white,\n    &.is-menu-open {\n\n        .header__btn-toggle {\n            background-color: #fff;\n        }\n\n        .header__logo {\n            svg path {\n                fill: #fff;\n            }\n        }\n    }\n}\n\n.header {\n    padding-top: 24px;\n    position: fixed;\n    top: 0;\n    left: 0;\n    width: 100%;\n    z-index: 500;\n\n    &__logo {\n        width: 50px;\n\n        svg path {\n            transition: all .6s ease-in-out;\n        }\n    }\n\n    &__row {\n        display: flex;\n        align-items: center;\n        justify-content: space-between;\n        position: relative;\n        z-index: 300;\n    }\n\n    &__btn-toggle {\n        -webkit-appearance: none;\n        appearance: none;\n        background: none;\n        border: none;\n        padding: 0;\n        margin: 0;\n        width: 30px;\n        height: 30px;\n        border-radius: 50%;\n        background-color: #333;\n        cursor: pointer;\n        transition: all .3s ease-in-out;\n        outline: none;\n\n        &:hover {\n            transform: scale(1.4);\n        }\n\n        >span {\n            position: fixed;\n            top: -99999px;\n            left: -99999px;\n            display: block;\n        }\n    }\n\n    &__menu-dropdown {\n        transition: 0.5s ease-in-out all;\n        opacity: 0;\n        visibility: hidden;\n        position: fixed;\n        width: 100%;\n        height: 100%;\n        top: 0;\n        left: 0;\n        background-color: rgba(1, 24, 50, .9);\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        z-index: 100;\n\n        nav {\n            width: 100%;\n        }\n\n        ul {\n            list-style-type: none;\n            font-family: \"Montserrat\", sans-serif;\n            letter-spacing: 8px;\n            font-size: 60px;\n            margin: 0;\n            padding: 0;\n            text-align: center;\n        }\n\n        li {\n            margin: 24px 0;\n        }\n\n        li a {\n            text-transform: uppercase;\n            color: #fff;\n            text-decoration: none;\n            display: block;\n        }\n    }\n}\n\n.home,\n.work {\n\n    position: absolute;\n    left: 0;\n    width: 100%;\n    height: 100vh;\n\n}\n\n.home {\n    top: 50px;\n}\n\n.home__content,\n.work__content {\n    display: flex;\n    align-items: center;\n    height: 100%;\n}\n\n.home {\n    &__wrapper {\n        width: 100%;\n    }\n\n    &__presentation {\n        max-width: 832px;\n    }\n\n    &__heading {\n        color: #333;\n        font-size: 120px;\n        line-height: 116px;\n        font-family: \"Montserrat\", sans-serif;\n        margin: 0;\n\n        span {\n            display: block;\n        }\n\n        span.light {\n            font-weight: 300;\n        }\n\n        @media(max-width: 844px) {\n            font-size: 90px;\n            line-height: 86px;\n        }\n    }\n\n    &__subline {\n        font-family: \"Source Code Pro\", monospace;\n        font-size: 23px;\n        text-decoration: underline;\n        margin-top: 28px;\n        display: block;\n        color: #333;\n    }\n\n    &__description {\n        font-family: \"Source Code Pro\", monospace;\n        font-size: 16px;\n        color: #333;\n        display: block;\n        margin-top: 12px;\n\n        .stuff {\n            position: relative;\n            padding: 0 24px;\n\n            &:before {\n                content: '';\n                position: absolute;\n                bottom: 0;\n                left: 0;\n                width: 100%;\n                height: 1px;\n                background-color: #333;\n            }\n        }\n\n        .bold {\n            font-weight: bold;\n        }\n    }\n\n    &__arrow {\n        margin-top: 40px;\n        text-align: center;\n    }\n}\n\n.work {\n\n    &__counter-container {\n        position: absolute;\n        top: 50%;\n        left: 40px;\n        transform: translateY(-50%);\n        color: #fff;\n        text-align: center;\n        z-index: 3;\n        font-family: \"Montserrat\", sans-serif;\n        font-size: 34px;\n        opacity: 0;\n        visibility: hidden;\n\n        transition: color .8s ease-in-out;\n\n        &.dark {\n            color: #333;\n        }\n    }\n\n    &__numbers {\n        display: flex;\n        align-items: flex-end;\n        justify-content: center;\n\n    }\n\n    &__pipe {\n        padding: 0 4px;\n    }\n\n    &__total {\n        font-size: 18px;\n    }\n\n    &__project-label {\n        font-size: 14px;\n        text-transform: uppercase;\n        margin-top: 12px;\n    }\n\n    &__presentation {\n        width: 100%;\n    }\n\n    &__slide {\n        display: flex;\n        align-items: center;\n        justify-content: space-between;\n        // background-color: #6653a6;\n        position: absolute;\n        top: 0;\n        left: 0;\n        width: 100%;\n        height: 100%;\n        padding: 0 78px;\n\n        &.dark {\n            .work__information {\n                color: #333;\n            }\n        }\n    }\n\n    &__picture {\n        max-width: 400px;\n\n        img {\n            max-width: 100%;\n            height: auto;\n        }\n    }\n\n\n\n    &__information {\n        flex: 1;\n        font-family: \"Montserrat\", sans-serif;\n        color: #fff;\n        padding-left: 24px;\n    }\n\n    &__type {\n        display: block;\n        font-size: 30px;\n    }\n\n    &__title {\n        display: block;\n        font-size: 60px;\n        font-weight: 600;\n    }\n\n    &__description {\n        font-family: \"Source Code Pro\", monospace;\n        font-weight: 300;\n    }\n\n    &__table {\n        width: 100%;\n\n        &-row {\n            display: flex;\n            padding-top: 10px;\n        }\n\n        &-column,\n        &-header {\n            flex: 1;\n        }\n\n        &-header {\n            font-weight: 600;\n        }\n    }\n\n    &__slider {\n        position: absolute;\n        top: 0;\n        left: 50%;\n        width: 100%;\n        height: 100%;\n        max-width: 1120px;\n        transform: translateX(-50%);\n    }\n\n}"},"lineCount":null}},"error":null,"hash":"b383cb9bef5dbd778b72e0e4b65a8e81","cacheData":{"env":{}}}